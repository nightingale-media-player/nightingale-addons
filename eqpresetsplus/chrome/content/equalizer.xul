<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE overlay SYSTEM "chrome://cometeeq/locale/overlay.dtd">
<!--

<!DOCTYPE overlay [
<!ENTITY % cometeeq SYSTEM "chrome://cometeeq/locale/overlay.dtd">
%cometeeq;
